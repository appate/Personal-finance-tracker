<div class="container">
<h1>Your Profile</h1>
    <form id="setting-form" action="/settings/profile" method="POST">
        <label for="firstname">First Name</label>
        <input type="text" id="firstname" name="firstname" required value={{firstname}}>
        <label for="lastname">Last Name</label>
        <input type="text" id="lastname" name="lastname" required value={{lastname}}>
        <label for="dob">Date of Birth</label>
        <input type="date" id="dob" name="dob" required value={{dob}}>
        <label for="email">Email Address</label>
        <input type="email" id="email" name="email" required value={{email}}>

        <button type="submit">Submit</button>
        <button onclick=window.location.href="/dashboard">Back to Dashboard</button>

    </form>
</div>
